    abstract = {School of Geography, University of Nottingham, Nottingham, UK Columbia River Fish and Wildlife Conservation Office, U.S. Fish and Wildlife Service, Vancouver, Washington Department Landscape Architecture and Environmental Planning, University of California, Berkeley, California CASM Environmental, LLC, Portland, Oregon Biological Sciences Department, University of Lethbridge, Lethbridge, Alberta, Canada Geography and Planning Department, University of Saskatchewan, Saskatoon, Saskatchewan, Canada },
